<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
    <link rel="stylesheet" href="build/output.css">
</head>
<body>
    <header class="fixed top-0 left-0 right-0 z-50 bg-black/30 backdrop-blur-md">
        <div class="container mx-auto px-6 py-4 flex items-center justify-between">
            <div class="flex items-center gap-2">
                <div class="text-white">
                    <div class="font-bold text-lg">MNR</div>
                    <div class="text-xs">PARTNERS</div>
                </div>
            </div>
            <nav class="flex items-center gap-8">
                {% for key, value in menu %}
                    <a href="#{{ key }}" class="text-gray-300 hover:text-white transition-colors">{{ value }}</a>
                {% endfor %}
            </nav>
            <a href="#contact" class="bg-blue-400 hover:bg-blue-500 transition-colors text-white px-6 py-2 rounded">Contact</a>
        </div>
    </header>

    <section class="relative h-screen flex items-center justify-center bg-cover bg-center bg-slate-50" style="background-image: url('{{ hero.image }}');">
        <div class="relative z-10 text-center px-6 max-w-4xl">
            <h1 class="text-5xl md:text-6xl font-bold text-foreground mb-6">{{ hero.title }}</h1>
            <p class="text-xl text-foreground/80">{{ hero.description }}</p>
        </div>
        <div class="absolute bottom-0 left-0 right-0 h-full w-full bg-white/20"></div>
    </section>

    <section id="why" class="py-24">
</body>
</html>